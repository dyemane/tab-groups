{
  "name": "tab-groups",
  "version": "0.2.0",
  "private": true,
  "description": "Save, restore, and switch tab groups per project",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc --noEmit && vite build",
    "test": "vitest run",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "package": "npm run build && cd dist && zip -r ../tab-groups-0.1.0.zip . && cd .. && echo 'Packaged: tab-groups-0.1.0.zip'"
  },
  "dependencies": {
    "preact": "^10.25.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@preact/preset-vite": "^2.10.0",
    "@types/chrome": "^0.0.287",
    "typescript": "^5.7.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.0"
  }
}
